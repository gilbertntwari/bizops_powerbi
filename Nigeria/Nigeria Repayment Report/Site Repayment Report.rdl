          <DataField>TotalCredit</DataField>
        </Field>
        <Field Name="Repaid">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Repaid</DataField>
        </Field>
        <Field Name="CompletedGroupsCount">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>CompletedGroupsCount</DataField>
        </Field>
        <Field Name="TGP">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>TGP</DataField>
        </Field>
        <Field Name="Farmers">
          <rd:TypeName>System.Int32</rd:TypeName>
